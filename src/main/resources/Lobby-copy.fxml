<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.cell.PropertyValueFactory?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>
<BorderPane xmlns:fx="http://javafx.com/fxml/1" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="450" minWidth="700"
            prefHeight="450.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/9"
            fx:controller="kr.rvs.kkutu.gui.LobbyController">
    <padding>
        <Insets bottom="10" left="10" right="10" top="10"/>
    </padding>
    <center>
        <SplitPane dividerPositions="0.25073746312684364" prefHeight="200.0" prefWidth="200.0"
                   BorderPane.alignment="CENTER">
            <items>
                <SplitPane dividerPositions="0.6518691588785047" orientation="VERTICAL" prefHeight="200.0"
                           prefWidth="174.0" SplitPane.resizableWithParent="false">
                    <items>
                        <TitledPane fx:id="titledUsersPane" collapsible="false" maxHeight="Infinity" prefHeight="0.0"
                                    prefWidth="0.0" text="Loading" SplitPane.resizableWithParent="false">
                            <content>
                                <SplitPane dividerPositions="0.5" orientation="VERTICAL" prefHeight="200.0"
                                           prefWidth="160.0">
                                    <items>
                                        <ListView fx:id="userView"/>
                                        <TextField fx:id="userSearchField" text="검색..."/>
                                    </items>
                                </SplitPane>
                            </content>
                        </TitledPane>
                        <VBox prefHeight="200.0" prefWidth="100.0" spacing="10.0">
                            <children>
                                <HBox prefHeight="57.0" prefWidth="167.0">
                                    <children>
                                        <ImageView fitHeight="87.0" fitWidth="87.0" pickOnBounds="true"
                                                   preserveRatio="true"/>
                                        <GridPane>
                                            <columnConstraints>
                                                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                                            </columnConstraints>
                                            <rowConstraints>
                                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                                            </rowConstraints>
                                            <children>
                                                <Label alignment="CENTER" maxWidth="1.7976931348623157E308" text="Label"
                                                       textAlignment="CENTER"/>
                                                <Label alignment="CENTER" maxWidth="1.7976931348623157E308" text="Label"
                                                       textAlignment="CENTER" GridPane.rowIndex="1"/>
                                                <Label alignment="CENTER" maxWidth="1.7976931348623157E308" text="Label"
                                                       textAlignment="CENTER" GridPane.rowIndex="2"/>
                                                <Label alignment="CENTER" maxWidth="1.7976931348623157E308" text="Label"
                                                       textAlignment="CENTER" GridPane.rowIndex="3"/>
                                            </children>
                                        </GridPane>
                                    </children>
                                </HBox>
                                <Label alignment="CENTER" maxWidth="1.7976931348623157E308" text="Loading..."
                                       textAlignment="CENTER"/>
                                <ProgressBar prefWidth="200.0" progress="0.0"/>
                            </children>
                        </VBox>
                    </items>
                </SplitPane>
                <SplitPane dividerPositions="0.5" orientation="VERTICAL" prefHeight="200.0" prefWidth="160.0"
                           SplitPane.resizableWithParent="false">
                    <items>
                        <SplitPane dividerPositions="0.5" mouseTransparent="true" orientation="VERTICAL"
                                   prefHeight="200.0" prefWidth="160.0">
                            <items>
                                <TableView fx:id="roomView">
                                    <columns>
                                        <TableColumn prefWidth="44.0" style="-fx-alignment: center" text="번호">
                                            <cellValueFactory>
                                                <PropertyValueFactory property="id"/>
                                            </cellValueFactory>
                                        </TableColumn>
                                        <TableColumn prefWidth="112.0" style="-fx-alignment: center" text="제목">
                                            <cellValueFactory>
                                                <PropertyValueFactory property="title"/>
                                            </cellValueFactory>
                                        </TableColumn>
                                        <TableColumn style="-fx-alignment: center" text="모드">
                                            <cellValueFactory>
                                                <PropertyValueFactory property="mode"/>
                                            </cellValueFactory>
                                        </TableColumn>
                                        <TableColumn style="-fx-alignment: center" text="라운드">
                                            <cellValueFactory>
                                                <PropertyValueFactory property="round"/>
                                            </cellValueFactory>
                                        </TableColumn>
                                        <TableColumn style="-fx-alignment: center" text="시간">
                                            <cellValueFactory>
                                                <PropertyValueFactory property="time"/>
                                            </cellValueFactory>
                                        </TableColumn>
                                        <TableColumn style="-fx-alignment: center" text="인원">
                                            <cellValueFactory>
                                                <PropertyValueFactory property="size"/>
                                            </cellValueFactory>
                                        </TableColumn>
                                        <TableColumn style="-fx-alignment: center" text="잠금">
                                            <cellValueFactory>
                                                <PropertyValueFactory property="password"/>
                                            </cellValueFactory>
                                        </TableColumn>
                                    </columns>

                                    <columnResizePolicy>
                                        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
                                    </columnResizePolicy>
                                </TableView>

                                <TextArea fx:id="chatArea" editable="false" prefHeight="200.0" prefWidth="200.0"
                                          wrapText="true"/>
                            </items>
                        </SplitPane>
                        <TextField fx:id="chatField" text="메세지..."/>
                    </items>
                </SplitPane>
            </items>
        </SplitPane>
    </center>
</BorderPane>
